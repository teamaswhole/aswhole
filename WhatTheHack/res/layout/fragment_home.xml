        android:background="@null"
        android:src="@drawable/pink" />

    <ImageButton
        android:id="@+id/btn9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn2"
        android:layout_alignTop="@+id/btn8"
        android:layout_marginTop="18dp"
        android:background="@null"
        android:src="@drawable/blue" />

    <ImageButton
        android:id="@+id/btn11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn9"
        android:layout_marginRight="19dp"
        android:layout_toLeftOf="@+id/btn13"
        android:background="@null"
        android:src="@drawable/yellow" />

    <ImageButton
        android:id="@+id/btn14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btn13"
        android:layout_marginBottom="16dp"
        android:layout_toRightOf="@+id/btn8"
        android:background="@null"
        android:src="@drawable/green" />

    <ImageButton
        android:id="@+id/btn17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn14"
        android:layout_below="@+id/btn14"
        android:background="@null"
        android:src="@drawable/blue" />

    <ImageButton
        android:id="@+id/btn15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn9"
        android:layout_alignTop="@+id/btn17"
        android:layout_marginTop="18dp"
        android:background="@null"
        android:src="@drawable/green" />

    <ImageButton
        android:id="@+id/btn5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn4"
        android:layout_marginRight="24dp"
        android:layout_toLeftOf="@+id/btn3"
        android:background="@null"
        android:src="@drawable/green" />

    <ImageButton
        android:id="@+id/btn10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btn11"
        android:layout_toLeftOf="@+id/btn3"
        android:background="@null"
        android:src="@drawable/pink" />

    <ImageButton
        android:id="@+id/btn4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn3"
        android:layout_toLeftOf="@+id/btn6"
        android:background="@null"
        android:src="@drawable/pink" />

    <ImageButton
        android:id="@+id/btn7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/btn6"
        android:layout_marginTop="17dp"
        android:layout_toLeftOf="@+id/btn16"
        android:background="@null"
        android:src="@drawable/violet2" />

    <ImageButton
        android:id="@+id/btn12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btn16"
        android:layout_toRightOf="@+id/btn15"
        android:background="@null"
        android:src="@drawable/yellow" />

    <ImageButton
        android:id="@+id/btn13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn11"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="16dp"
        android:layout_toRightOf="@+id/btn12"
        android:background="@null"
        android:src="@drawable/yellow2" />

    <ImageButton
        android:id="@+id/btn16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn13"
        android:layout_alignTop="@+id/btn15"
        android:background="@null"
        android:src="@drawable/violet2" />

    <ImageButton
        android:id="@+id/btn6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/btn5"
        android:layout_toLeftOf="@+id/btn11"
        android:background="@null"
        android:src="@drawable/yellow" />

</RelativeLayout>
